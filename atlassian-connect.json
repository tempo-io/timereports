{
    "key": "timereports",
    "name": "Time Reports for Jira Cloud",
    "description": "Provides miscellaneous views for Jira time tracking data",
    "enableLicensing": true,
    "vendor": {
        "name": "Andriy Zhdanov",
        "url": "https://www.primetimesheet.net/timereports"
    },
    "baseUrl": "https://timereports.github.io/",
    "links": {
        "self": "https://timereports.github.io/atlassian-connect.json",
        "homepage": "https://timereports.github.io/atlassian-connect.json"
    },
    "authentication": {
        "type": "none"
    },
    "modules": {
        "generalPages": [{
            "location": "find_link/timesheet-menu",
            "name": {
                "value": "Time Reports"
            },
            "url": "/timereports.html?project.key={project.key}&v=2",
            "key": "timereports",
            "conditions": [{
                "condition": "user_is_logged_in"
            }]
        }, {
            "name": {
                "value": "Time Reports"
            },
            "icon": {
                "width": 16,
                "height": 16,
                "url": "data:image/svg+xml;base64,"
            },
            "key": "timereports-system",
            "url": "/timereports.html?v=3",
            "cacheable": true,
            "location": "system.top.navigation.bar",
            "conditions": [{
                  "or": [{
                      "condition": "addon_property_contains_any_user_group",
                      "params": {
                          "propertyKey": "configuration",
                          "objectName": "timeentryPanelGroups"
                      }
                  }, {
                      "condition": "addon_property_contains_any_user_group",
                      "params": {
                          "propertyKey": "configuration",
                          "objectName": "auditorsGroups"
                      }
                  }]
              }, {
                  "condition": "user_is_logged_in"
            }]
        }],
        "configurePage": {
            "name": {
                "value": "Time Reports Configuration"
            },
            "url": "/timereports.html?configuration=true&v=2",
            "key": "configuration",
            "conditions": [{
                "condition": "user_is_logged_in"
            }]
        },
        "postInstallPage": {
            "name": {
                "value": "Welcome to Time Reports and Gadgets Add-on"
            },
            "url": "/timereports.html?welcome=true&v=2",
            "key": "welcome",
            "conditions": [{
                "condition": "user_is_logged_in"
            }]
        },
        "jiraReports": [{
            "url": "timereports.html?project.key={project.key}&v=2",
            "weight": 100,
            "description": {
              "value": "Flexible view of issues time tracking data"
            },
            "reportCategory": "other",
            "thumbnailUrl": "/images/timesheet-report-thumb.png",
            "key": "timereports-report",
            "name": {
              "value": "Time Reports"
            }
         }],
        "jiraDashboardItems": [{
            "name" : {
                "value" : "Time Reports"
            },
            "url" : "/timereports.html?project.key={project.key}&dashboard={dashboard.id}&dashboardItem={dashboardItem.id}&v=2",
            "key" : "timereports-gadget",
            "description" : {
                "value" : "Flexible view of issues time tracking data."
            },
            "thumbnailUrl": "/images/timesheet-gadget-thumb.png",
            "configurable" : true,
            "conditions": [{
                "condition": "user_is_logged_in"
            }]
        }],
        "jiraIssueContents": [
          {
            "icon": {
              "width": 0,
              "height": 0,
              "url": "data:image/svg+xml;base64,"
            },
            "target": {
              "type": "web_panel",
              "url": "/timereports.html?issue.key={issue.key}&issue.id={issue.id}&v=3"
            },
            "tooltip": {
              "value": "Log work done"
            },
            "name": {
              "value": "Time Entry"
            },
            "conditions": [
              {
                "and": [
                  {
                    "condition": "addon_property_contains_any_user_group",
                    "params": {
                      "propertyKey": "configuration",
                      "objectName": "timeentryPanelGroups"
                    }
                  },
                  {
                    "condition": "has_issue_permission",
                    "params": {
                      "permission": "WORK_ON_ISSUES"
                    }
                  }
                ]
              }
            ],
            "key": "timeentry-issue-content"
          }
        ],
        "webPanels": [
          {
            "url": "/timereports.html?issue.key={issue.key}&issue.id={issue.id}&v=3",
            "location": "atl.jira.view.issue.left.context",
            "layout": {
              "width": "10px",
              "height": "100%"
            },
            "weight": 500,
            "name": {
              "value": "Time Entry"
            },
            "conditions": [
              {
                "and": [
                  {
                    "condition": "addon_property_contains_any_user_group",
                    "params": {
                      "propertyKey": "configuration",
                      "objectName": "timeentryPanelGroups"
                    }
                  },
                  {
                    "condition": "has_issue_permission",
                    "params": {
                      "permission": "WORK_ON_ISSUES"
                    }
                  }
                ]
              }
            ],
            "key": "timeentry-panel"
          }
        ],
        "webItems" : [{
            "name": {
                "value": "Delete Worklog"
            },
            "key": "deleteworklog",
            "url": "/timereports.html?dialog=deleteworklog&v=2",
            "location": "none",
            "context": "addon",
            "target": {
                "type": "dialog"
            }
        }, {
            "name": {
                "value": "Update Worklog"
            },
            "key": "updateworklog",
            "url": "/timereports.html?dialog=updateworklog&v=2",
            "location": "none",
            "context": "addon",
            "target": {
                "type": "dialog"
            }
        }, {
            "name": {
                "value": "Create Worklog"
            },
            "key": "createworklog",
            "url": "/timereports.html?dialog=createworklog&v=2",
            "location": "none",
            "context": "addon",
            "target": {
                "type": "dialog"
            }
        }, {
            "name": {
                "value": "Add Worklog"
            },
            "key": "addworklog",
            "url": "/timereports.html?dialog=addworklog&v=2",
            "location": "none",
            "context": "addon",
            "target": {
                "type": "dialog"
            }
        }, {
            "key": "timereports-search",
            "location": "jira.navigator.pluggable.items",
            "weight": 9999,
            "name": {
                "value": "Time Reports"
            },
            "icon": {
                "width": 16,
                "height": 16,
                "url": "data:image/svg+xml;base64,"
            },
            "context": "addon",
            "url": "/timereports.html?dialog=timereports&v=2",
            "target": {
                "type": "dialog",
                "options": {
                    "header": {
                        "value": "Time Reports"
                    },
                    "width": "320px"
                }
            }
        }, {
            "name": {
                "value": "Report Problem"
            },
            "key": "bitbucket",
            "url": "/timereports.html?dialog=bitbucket&v=2",
            "location": "none",
            "context": "addon",
            "target": {
                "type": "dialog"
            }
        }]
    },
    "scopes": [
        "READ",
        "WRITE",
        "DELETE"
    ]
}
