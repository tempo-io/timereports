{
    "key": "timereports",
    "name": "Time Reports for Jira Cloud",
    "description": "Provides miscellaneous views for Jira time tracking data",
    "enableLicensing": true,
    "vendor": {
        "name": "Prime Timesheet",
        "url": "https://www.primetimesheet.net/timereports"
    },
    "baseUrl": "https://timereports.primetimesheet.net/",
    "links": {
        "self": "https://timereports.primetimesheet.net/atlassian-connect.json",
        "homepage": "https://timereports.primetimesheet.net/atlassian-connect.json"
    },
    "authentication": {
        "type": "none"
    },
    "modules": {
        "adminPages": [{
            "url": "/timereports.html?configuration=true&v=2",
            "name": {
                "value": "Time Reports Configuration",
                "i18n": "Time Reports Configuration"
            },
            "location": "admin_plugins_menu/timetracking_section",
            "key": "timereports-time-tracking-config"
        }],
        "jiraTimeTrackingProviders": [{
            "key": "timereports-time-tracking-provider",
            "name": {
                "value": "Time Reports provided time tracking"
            },
            "adminPageKey": "timereports-time-tracking-config"
        }],
        "generalPages": [{
            "location": "find_link/timesheet-menu",
            "name": {
                "value": "Time Reports",
                "i18n": "Time Reports"
            },
            "url": "/timereports.html?v=2",
            "cacheable": true,
            "key": "timereports",
            "conditions": [{
                "condition": "user_is_logged_in"
            }]
        }, {
            "name": {
                "value": "Time Reports",
                "i18n": "Time Reports"
            },
            "icon": {
                "width": 16,
                "height": 16,
                "url": "data:image/svg+xml;base64,"
            },
            "key": "timereports-system",
            "url": "/timereports.html?v=2",
            "cacheable": true,
            "location": "system.top.navigation.bar",
            "conditions": [{
                "or": [{
                    "condition": "addon_property_contains_any_user_group",
                    "params": {
                        "propertyKey": "configuration",
                        "objectName": "timeentryPanelGroups"
                    }
                }, {
                    "condition": "addon_property_contains_any_user_group",
                    "params": {
                        "propertyKey": "configuration",
                        "objectName": "auditorsGroups"
                    }
                }]
            }, {
                "condition": "user_is_logged_in"
            }]
        }],
        "jiraProjectPages": [{
            "key": "timereports-project",
            "name": {
                "value": "Time Reports",
                "i18n": "Time Reports"
            },
            "url": "/timereports.html?v=2",
            "cacheable": true,
            "iconUrl": "data:image/svg+xml;base64,",
            "weight": 1,
            "conditions": [{
                "or": [{
                    "condition": "addon_property_contains_any_user_group",
                    "params": {
                        "propertyKey": "configuration",
                        "objectName": "timeentryPanelGroups"
                    }
                }, {
                    "condition": "addon_property_contains_context",
                    "params": {
                        "propertyKey": "configuration",
                        "objectName": "timeentryPanelProjects",
                        "contextParameter": "project.key"
                    }
                }, {
                    "condition": "addon_property_contains_any_user_group",
                    "params": {
                        "propertyKey": "configuration",
                        "objectName": "auditorsGroups"
                    }
                }]
            }, {
                "condition": "user_is_logged_in"
            }]
        }],
        "configurePage": {
            "name": {
                "value": "Time Reports Configuration",
                "i18n": "Time Reports Configuration"
            },
            "url": "/timereports.html?configuration=true&v=2",
            "cacheable": true,
            "key": "configuration",
            "conditions": [{
                "condition": "user_is_logged_in"
            }]
        },
        "postInstallPage": {
            "name": {
                "value": "Welcome to Time Reports",
                "i18n": "Welcome to Time Reports"
            },
            "url": "/timereports.html?welcome=true&v=2",
            "cacheable": true,
            "key": "welcome",
            "conditions": [{
                "condition": "user_is_logged_in"
            }]
        },
        "jiraReports": [{
            "url": "timereports.html?v=2",
            "cacheable": true,
            "weight": 100,
            "description": {
                "value": "Flexible view of issues time tracking data",
                "i18n": "Flexible view of issues time tracking data"
            },
            "reportCategory": "other",
            "thumbnailUrl": "/images/timesheet-report-thumb.png",
            "key": "timereports-report",
            "name": {
                "value": "Time Reports",
                "i18n": "Time Reports"
            }
        }],
        "jiraDashboardItems": [{
            "name" : {
                "value" : "Time Reports",
                "i18n": "Time Reports"
            },
            "url" : "/timereports.html?v=2",
            "cacheable": true,
            "key" : "timereports-gadget",
            "description" : {
                "value" : "Flexible view of issues time tracking data.",
                "i18n": "Flexible view of issues time tracking data"
            },
            "thumbnailUrl": "/images/timesheet-gadget-thumb.png",
            "configurable" : true,
            "conditions": [{
                "condition": "user_is_logged_in"
            }]
        }],
        "jiraIssueContents": [{
            "icon": {
                "width": 0,
                "height": 0,
                "url": "data:image/svg+xml;base64,"
            },
            "target": {
                "type": "web_panel",
                "url": "/timereports.html?v=2"
            },
            "cacheable": true,
            "tooltip": {
                "value": "Log Work Done",
                "i18n": "Log Work Done"
            },
            "name": {
                "value": "Time Entry",
                "i18n": "Time Entry"
            },
            "conditions": [{
                "or": [{
                    "condition": "addon_property_contains_any_user_group",
                    "params": {
                        "propertyKey": "configuration",
                        "objectName": "timeentryPanelGroups"
                    }
                }, {
                    "condition": "addon_property_contains_context",
                    "params": {
                        "propertyKey": "configuration",
                        "objectName": "timeentryPanelProjects",
                        "contextParameter": "project.key"
                    }
                }]
            }, {
                "condition": "has_issue_permission",
                "params": {
                    "permission": "WORK_ON_ISSUES"
                }
            }],
            "contentPresentConditions": [{
              "condition": "user_is_logged_in"
            }],
            "key": "timeentry-issue-content"
        }
        ],
        "jiraIssueTabPanels": [
            {
                "url": "/timereports.html?issuetimebalance=true&v=2",
                "cacheable": true,
                "weight": 100,
                "name": {
                    "value": "Time Balance",
                    "i18n": "Time Balance"
                },
                "key": "timebalance-tab",
                "conditions": [
                    {
                        "condition": "addon_time_tracking_provider_enabled",
                        "params": {
                            "addonKey": "timereports",
                            "moduleKey": "timereports-time-tracking-provider"
                        }
                    }
                ]
            }
        ],
        "webPanels": [
            {
                "url": "/timereports.html?v=2",
                "cacheable": true,
                "location": "atl.jira.view.issue.left.context",
                "layout": {
                    "width": "10px",
                    "height": "100%"
                },
                "weight": 500,
                "name": {
                    "value": "Time Entry",
                    "i18n": "Time Entry"
                },
                "conditions": [{
                    "or": [{
                        "condition": "addon_property_contains_any_user_group",
                        "params": {
                            "propertyKey": "configuration",
                            "objectName": "timeentryPanelGroups"
                        }
                    }, {
                        "condition": "addon_property_contains_context",
                        "params": {
                            "propertyKey": "configuration",
                            "objectName": "timeentryPanelProjects",
                            "contextParameter": "project.key"
                        }
                    }]
                }, {
                    "condition": "has_issue_permission",
                    "params": {
                        "permission": "WORK_ON_ISSUES"
                    }
                }],
                "key": "timeentry-panel"
            }
        ],
        "webItems" : [{
            "key": "timereports-search",
            "location": "jira.navigator.pluggable.items",
            "weight": 9999,
            "name": {
                "value": "Time Reports",
                "i18n": "Time Reports"
            },
            "tooltip": {
                "value": "Time Reports",
                "i18n": "Time Reports"
            },
            "icon": {
                "width": 16,
                "height": 16,
                "url": "data:image/svg+xml;base64,"
            },
            "cacheable": true,
            "context": "addon",
            "url": "/timereports.html?dialog=timereports&v=2",
            "target": {
                "type": "dialog",
                "options": {
                    "header": {
                        "value": "Time Reports",
                        "i18n": "Time Reports"
                    },
                    "width": "320px"
                }
            }
        }, {
            "name": {
                "value": "Report Problem",
                "i18n": "Report Problem"
            },
            "key": "bitbucket",
            "url": "/timereports.html?dialog=bitbucket&v=2",
            "cacheable": true,
            "location": "none",
            "context": "addon",
            "target": {
                "type": "dialog"
            }
        }, {
            "key": "log-work",
            "location": "operations-work",
            "weight": 100,
            "name": {
                "value": "Log work",
                "i18n": "Log work"
            },
            "url": "/timereports.html?dialog=createworklog&v=2",
            "cacheable": true,
            "context": "addon",
            "target": {
                "type": "dialog",
                "options": {
                    "header": {
                        "value": "Log Work Done",
                        "i18n": "Log Work Done"
                    },
                    "height": "300px",
                    "width": "650px"
                }
            },
            "conditions": [{
                "condition": "addon_time_tracking_provider_enabled",
                "params": {
                    "addonKey": "timereports",
                    "moduleKey": "timereports-time-tracking-provider"
                }
            }, {
                "condition": "addon_property_equal_to",
                "invert": true,
                "params": {
                    "propertyKey": "configuration",
                    "objectName": "disableLogWork",
                    "value": "true"
                }
            }]
        }]
    },
    "apiMigrations": {
        "gdpr": true
    },
    "translations": {
        "paths": {
            "ru-RU": "/i18n/descriptor_ru.json",
            "fr-FR": "/i18n/descriptor_fr.json",
            "de-DE": "/i18n/descriptor_de.json",
            "es-ES": "/i18n/descriptor_es.json"
        }
    },
    "scopes": [
        "READ",
        "WRITE",
        "DELETE"
    ]
}
